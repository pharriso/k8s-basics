apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: blog
  labels:
    app: blog
spec:
  rules:
  - host: blog-${PROJECT_NAMESPACE}.${CLUSTER_SUBDOMAIN}
    http:
      paths:
      - path: "/"
        pathType: Prefix
        backend:
          service:
            name: blog
            port: 
             number: 8080
